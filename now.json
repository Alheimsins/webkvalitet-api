{
  "version": 2,
  "name": "webkvalitet-api",
  "scope": "alheimsins",
  "alias": [
    "webkvalitet.api.alheimsins.net"
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "data" }
    }
  ],
  "routes": [
    { "src": "/direktorater",
      "dest": "/direktorater.json",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept"
      }
    },
    { "src": "/partier",
      "dest": "/partier.json",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept"
      }
    },
    { "src": "/fylker",
      "dest": "/fylker.json",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept"
      }
    },
    { "src": "/smartbyer",
      "dest": "/smartbyer.json",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept"
      }
    }
  ]
}